RewriteEngine on

#Route de users =============================================





#LOGOUT
#Pattern: users/logout
#Target : index.php/?users=logout

rewriteRule ^users/logout$ index.php/?users=logout [L]

#Route par defaut =============================================


#route defaut
#Pattern: /
#Target : /

RewriteCond %{REQUEST_URI} !\.(js|css|jpg|gif|zip|png|pdf)$ [NC]